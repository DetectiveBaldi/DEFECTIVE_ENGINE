package core;

class Paths
{
    public static function mp3(key:String):String
    {
        return '${key}.mp3';
    }

    public static function ogg(key:String):String
    {
        return '${key}.ogg';
    }

    public static function png(key:String):String
    {
        return '${key}.png';
    }

    public static function xml(key:String):String
    {
        return '${key}.xml';
    }
}